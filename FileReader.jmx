              <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()
def json2 = new JsonSlurper().parseText(response)

assert json2.errorMessage != null : &quot;La respuesta JSON no contiene el valor esperado&quot;

log.info(props.get(&quot;myVarProperties&quot;))

assert response.contains(&quot;Wrong passwor&quot;) : &quot;no tiene een la respuesta&quot;

def obtenerHeader = prev.getResponseHeaders()

def nombrehead = &quot;X-Cloud-Trace-Context&quot;
def headerValue = null

obtenerHeader.eachLine { line -&gt;
    if (line.startsWith(nombrehead)) {
        headerValue = line.split(&apos;:&apos;)[1].trim()
    }
}
props.put(&quot;XCloudTraceContext&quot;, headerValue)</stringProp>
