            <stringProp name="script">import groovy.json.JsonOutput
import groovy.json.JsonSlurper

props.put(&quot;groovy.use.classvalue&quot;, &quot;true&quot;)
props.put(&quot;myVarProperties&quot;, &quot;constante&quot;)

def list_data = []
def file = new File(&apos;/Users/isaac/Desktop/CertificacionMaterialEEstudio/users.csv&apos;)
file.splitEachLine(&quot;,&quot;) { fields -&gt;
    def type = fields[3]
    if (type == &quot;debito&quot;) {
        def data = [
            user  : fields[0],
            passs : fields[1],
            number: fields[2]
        ]

        def json_data = JsonOutput.toJson(data)
        list_data.add(json_data)
    }
}

for (i in 0..&lt;list_data.size()) {
    def json = new JsonSlurper().parseText(list_data[i])
    log.info(&quot;Tarjeta de cr√©dito ${i + 1}: &quot; + json.number)
}

vars.putObject(&quot;credit_cards&quot;, list_data)
log.info(list_data[1])
